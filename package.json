{
  "name": "react-schema-render",
  "version": "0.0.3",
  "description": "react-schema-render 是一个通用型 schema 转 React 组件的工具组件。",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.min.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --write \"src/**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "keywords": [
    "react-schema-render",
    "react schema render",
    "json schema",
    "schema renderer",
    "react",
    "renderer",
    "render",
    "schema"
  ],
  "homepage": "https://dream2023.gitee.io/react-schema-render/",
  "bugs": "https://github.com/dream2023/react-schema-render/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dream2023/react-schema-render"
  },
  "author": {
    "name": "dream2023",
    "email": "1098626505@qq.com"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@umijs/plugin-antd": "^0.9.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.0.13",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
